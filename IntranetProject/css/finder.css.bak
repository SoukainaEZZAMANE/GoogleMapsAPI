  .ui-btn-up-f, .ui-btn-hover-f, .ui-btn-down-f {
  color: white;
  font-weight: bold;
  text-decoration: none; }

.ui-btn-up-f {
  border: 1px solid #711414;
  background:#1a3540;
  text-shadow: 0 -1px 1px #1a3540;
  background-image: -moz-linear-gradient(top, #1a3540, #1a3540);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #1a3540), color-stop(1, #1a3540));
  -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#c44f4f', EndColorStr='#ab2525')"; }

.ui-btn-hover-f {
  border: 1px solid #1a3540;
  background: #1a3540;
  text-shadow: 0 -1px 1px #1a3540;
  background-image: -moz-linear-gradient(top, #1a3540, #1a3540);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #1a3540), color-stop(1, #1a3540));
  -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#d47272', EndColorStr='#b54a4a')"; }

.ui-btn-down-f {
  border: 1px solid #1a3540;
  background: #1a3540;
  text-shadow: 0 -1px 1px #1a3540;
  background-image: -moz-linear-gradient(top, #1a3540, #1a3540);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #1a3540), color-stop(1, #1a3540));
  -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#9e3939', EndColorStr='#c44f4f')"; }
  
